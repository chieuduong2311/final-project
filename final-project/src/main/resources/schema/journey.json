{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Journey",
  "description": "Journey request",
  "type": "object",
  "properties": {
    "id": {
      "description": "The unique identifier for a call",
      "type": ["number", "null"]
    },
    "customerId": {
      "type": "number"
    },
    "callId": {
      "type": "number"
    },
    "driverId": {
      "type": ["number", "null"]
    },
    "status": {
      "type": ["string", "null"]
    },
    "reason": {
      "type": ["string", "null"]
    },
    "rate": {
      "type": ["number", "null"]
    },
    "origin": {
      "type": "object",
      "properties": {
        "id": {
          "type": ["integer", "null"]
        },
        "lat": {
          "type": "number"
        },
        "lng": {
          "type": "number"
        },
        "placeId": {
          "type": "string"
        },
        "placeName": {
          "type": "string"
        },
        "fullAddressInString": {
          "type": "string"
        }
      }
    },
    "destination": {
      "type": "object",
      "properties": {
        "id": {
          "type": ["integer", "null"]
        },
        "lat": {
          "type": "number"
        },
        "lng": {
          "type": "number"
        },
        "placeId": {
          "type": "string"
        },
        "placeName": {
          "type": "string"
        },
        "fullAddressInString": {
          "type": "string"
        }
      }
    },
    "startDateTime": {
      "type": ["integer", "null"]
    },
    "endDateTime": {
      "type": ["integer", "null"]
    },
    "price": {
      "type": ["number", "null"]
    },
    "paymentMethod": {
      "type": "string"
    },
    "pointCode": {
      "type": ["string", "null"]
    }
  },
  "required": ["customerId", "callId", "origin", "destination", "paymentMethod"]
}